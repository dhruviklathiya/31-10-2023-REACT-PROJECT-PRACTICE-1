t React, { useState } from 'react'\nimport \"./App7.css\"\nimport product_data from './product_data'\n\nconst App7 = () => {\n    const [finaldata,setfinaldata] = useState(product_data)\n    console.log(finaldata)\n    const input_handler = (e) => {\n        const price = Number(e.target.value)\n        setfinaldata( product_data?.filter((one)=> one.product_price <= price))\n    }\n    return (\n        <>\n            <div id=\"wrapper\" class=\"toggled\">\n                <div id=\"sidebar-wrapper\">\n                    <ul class=\"sidebar-nav\">\n                        <li class=\"sidebar-brand\">\n                            <a>\n                               Price filter\n                            </a>\n                        </li>\n                        <li>\n                            <input type='number' onChange={input_handler}/>\n                        </li>\n                    </ul>\n                </div>\n\n                <div id=\"page-content-wrapper\">\n                    <div class=\"container-fluid\">\n                        {finaldata.map((val_)=>{\n                            return(\n                                <>\n                                <div class=\"card\" style={{width: \"18rem\"}}>\n                                  <img class=\"card-img-top\" src={val_.product_image} alt=\"Card image cap\"/>\n                                  <div class=\"card-body\">\n                                    <h5 class=\"card-title\">{val_.product_name}</h5>\n                                    <p class=\"card-text\">{val_.product_desc}</p>\n                                    <p class=\"card-text\">${val_.product_price}</p>\n                                  </div>\n                                </div>\n                                </>\n                            )\n                        })}\n                    </div>\n                </div>\n            </div>\n        </>\n    )\n}\n\nexport default App7","/Users/dhruviklathiya/Desktop/D CODER/09 React js/Project_practice/31-10-2023-REACT-PROJECT-PRACTICE-1/src/product_data.js",[],[],"/Users/dhruviklathiya/Desktop/D CODER/09 React js/Project_practice/31-10-2023-REACT-PROJECT-PRACTICE-1/src/App8.jsx",[],[],"/Users/dhruviklathiya/Desktop/D CODER/09 React js/Project_practice/31-10-2023-REACT-PROJECT-PRACTICE-1/src/App9.jsx",[],[],"/Users/dhruviklathiya/Desktop/D CODER/09 React js/Project_practice/31-10-2023-REACT-PROJECT-PRACTICE-1/src/App10.jsx",[],[],"/Users/dhruviklathiya/Desktop/D CODER/09 React js/Project_practice/31-10-2023-REACT-PROJECT-PRACTICE-1/src/App11.jsx",["199","200"],[],"/Users/dhruviklathiya/Desktop/D CODER/09 React js/Project_practice/31-10-2023-REACT-PROJECT-PRACTICE-1/src/App12.jsx",["201","202","203"],[],"import axios from 'axios'\nimport React, { useEffect, useReducer, useState } from 'react'\n\nconst App12 = () => {\n    const [input,setInput] = useState({})\n    const [updateinput,setUpdateInput] = useState({})\n    const [data,setData] = useState([])\n    const [index,setIndex] = useState()\n    let index_ = 0\n    useEffect(()=>{\n        axios.get(\"http://localhost:3001/posts\").then((res)=>{\n            setData(res.data)\n        })\n    },[])\n    const input_handler = (e) => {\n        setInput({...input, [e.target.name]:e.target.value})\n    }\n    const submit_handler = () => {\n        axios.post(\"http://localhost:3001/posts\",input).then((res)=>{\n            setData([...data,res.data]);\n        })\n    }\n    const delete_handler = (id) => {\n        axios.delete(`http://localhost:3001/posts/${id}`).then((res)=>{\n            setData(data.filter((one_)=>one_.id !== id))\n        })\n    }\n    const input_handler_update = (e) => {\n        setUpdateInput({...updateinput, [e.target.name]:e.target.value})\n    }\n    const update_handler1 = (ind) => {\n        setUpdateInput(data[ind])\n        index_ = ind\n    }\n    const update_handler2 = async() => {\n        await axios.put(`http://localhost:3001/posts/${updateinput.id}`,updateinput).then((res)=>{\n            // Virtual dom update without API calling [using state]\n            data.splice(index,1,res.data)\n            setData([...data])\n            // Virtual dom update without API calling [without using state]\n            // data.splice(index_,1,res.data)\n            // setData([...data])\n        })\n    }\n    return (\n    <>\n    <div>\n    <input name='title' onChange={input_handler}/>\n    <input name='author' onChange={input_handler}/>\n    <button type='button' onClick={submit_handler}>Add data</button>\n    </div>\n    <div>\n    <input name='title' value={updateinput.title} onChange={input_handler_update}/>\n    <input name='author' value={updateinput.author} onChange={input_handler_update}/>\n    <button type='button' onClick={update_handler2}>Update</button>\n    {/* <button type='button' onClick={cancel_handler}>Cancel</button> */}\n    {/* <button type='button' onClick={}>Re-render</button> */}\n    </div>\n    {\n        data?.map((val,ind)=>{\n            return(\n                <>\n                <h1>{val.title}</h1>\n                <h2>{val.author}</h2>\n                <button onClick={()=>{delete_handler(val.id)}}>Delete</button>\n                <button onClick={()=>{update_handler1(ind)}}>Update</button>\n                </>\n            )\n        })\n    }\n    </>\n  )\n}\n\nexport default App12",["204"],"/Users/dhruviklathiya/Desktop/D CODER/09 React js/Project_practice/31-10-2023-REACT-PROJECT-PRACTICE-1/src/App13.jsx",["205","206","207"],[],"/Users/dhruviklathiya/Desktop/D CODER/09 React js/Project_practice/31-10-2023-REACT-PROJECT-PRACTICE-1/src/API/api.js",[],[],"/Users/dhruviklathiya/Desktop/D CODER/09 React js/Project_practice/31-10-2023-REACT-PROJECT-PRACTICE-1/src/Constant.js",[],[],"/Users/dhruviklathiya/Desktop/D CODER/09 React js/Project_practice/31-10-2023-REACT-PROJECT-PRACTICE-1/src/App14.jsx",["208"],[],"/Users/dhruviklathiya/Desktop/D CODER/09 React js/Proj